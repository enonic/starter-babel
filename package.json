  "scripts": {
    "build": "./gradlew build;",
    "clean": "rm -r build dist 2>/dev/null; true;",
    "deploy": "npm run install-app;",
    "doctoc": "./node_modules/.bin/doctoc --github ./README.md",
    "gulp": "./node_modules/.bin/gulp",
    "gulp:tasks": "./node_modules/.bin/gulp --tasks-simple;",
    "gulper": "./node_modules/.bin/gulper",
    "gdev": "npm run gulp dev;",
    "gprod": "npm run gulp prod;",
    "gwatch": "npm run gulper watch;",
    "install-app": "echo build/libs/*.jar|xargs -n1 $XP_INSTALL/toolbox/toolbox.sh install-app -a su:password -f ;",
    "xp": "$XP_INSTALL/bin/server.sh;",
    "xp:dev": "$XP_INSTALL/bin/server.sh dev;",
    "test:build": "NODE_ENV=test ./node_modules/.bin/mocha 'test/build.spec.es6'",
    "toolbox": "$XP_INSTALL/toolbox/toolbox.sh",
    "watch": "npm run gulper watch;",
    "yarn": "./node_modules/.bin/yarn"
  },
